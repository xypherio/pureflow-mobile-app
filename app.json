{
  "expo": {
    "name": "PureFlow",
    "slug": "pureflow-mobile",
    "version": "2.2.2",
    "owner": "xy_pher",
    "orientation": "portrait",
    "icon": "./assets/logo/pureflow-logo-apk-08.png",
    "scheme": "pureflow",
    "splash": {
      "image": "./assets/logo/logo-emblem.png",
      "resizeMode": "contain",
      "backgroundColor": "#002d66"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/pureflow-logo-apk-08.png",
        "backgroundColor": "#002d66"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.xypher.pureflowmobile",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "notification": {
      "icon": "./assets/logo/logo-emblem.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new interactions"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo/logo-emblem.png",
          "color": "#002d66",
          "sounds": ["./assets/water-alert.mp3"]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo-emblem.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#002d66",
          "shadowColor": "none"
        }
      ],
      "expo-web-browser",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "splashScreen": {
              "backgroundColor": "#002d66"
            },
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "useFrameworks": "static",
            "bundleIdentifier": "com.pureflow.mobile"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "GEMINI_API_KEY": "AIzaSyA1J4Ue-GM0pJT2rcILsJD52tTyuRbmuO0",
      "GEMINI_BASE_URL": "https://generativelanguage.googleapis.com/v1beta",
      "GEMINI_MODEL": "gemini-2.0-flash",
      "firebase": {
        "apiKey": "AIzaSyBCcrWxgO0wRa0-QL0DTLdExN01tbQrzhY",
        "authDomain": "pureflow-system.firebaseapp.com",
        "projectId": "pureflow-system",
        "storageBucket": "pureflow-system.appspot.com",
        "messagingSenderId": "817474825207",
        "appId": "1:817474825207:web:cfb188a35d073158575cf2",
        "measurementId": "G-T7X7JB8428"
      },
      "router": {},
      "eas": {
        "projectId": "d24fac03-1b41-4b93-ab04-8593c0f6095d"
      }
    }
  }
}
