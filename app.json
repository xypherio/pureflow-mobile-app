{
  "expo": {
    "name": "PureFlow",
    "slug": "pureflow-mobile",
    "version": "1.0.0",
    "owner": "xy_pher",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png",
    "scheme": "pureflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#002d66"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.xypher.pureflowmobile"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#ffffff"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo-emblem.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo-emblem.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#002d66"
        }
      ],
      "expo-web-browser",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "useFrameworks": "static",
            "bundleIdentifier": "com.pureflow.mobile"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "GEMINI_API_KEY": "AIzaSyA1J4Ue-GM0pJT2rcILsJD52tTyuRbmuO0",
      "GEMINI_BASE_URL": "https://generativelanguage.googleapis.com/v1beta",
      "GEMINI_MODEL": "gemini-2.0-flash",
      "firebase": {
        "apiKey": "AIzaSyBCcrWxgO0wRa0-QL0DTLdExN01tbQrzhY",
        "authDomain": "pureflow-system.firebaseapp.com",
        "projectId": "pureflow-system",
        "storageBucket": "pureflow-system.appspot.com",
        "messagingSenderId": "817474825207",
        "appId": "1:817474825207:web:cfb188a35d073158575cf2",
        "measurementId": "G-T7X7JB8428"
      },
      "router": {},
      "eas": {
        "projectId": "d24fac03-1b41-4b93-ab04-8593c0f6095d"
      }
    }
  }
}
